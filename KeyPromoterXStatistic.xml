<application>
  <component name="KeyPromoterXStatistic">
    <option name="statistics">
      <Statistic Action="Generate...">
        <Item>
          <IdeaActionID>Generate</IdeaActionID>
          <ShortCut>'Alt+Einfg'</ShortCut>
          <Description>Generate...</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Settings...">
        <Item>
          <IdeaActionID>ShowSettings</IdeaActionID>
          <ShortCut>'Strg+Alt+S'</ShortCut>
          <Description>Settings...</Description>
          <Count>7</Count>
        </Item>
      </Statistic>
      <Statistic Action="Terminal">
        <Item>
          <IdeaActionID>ActivateTerminalToolWindow</IdeaActionID>
          <ShortCut>'Strg+Alt+1'</ShortCut>
          <Description>Terminal</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
      <Statistic Action="Unit Tests">
        <Item>
          <IdeaActionID>ActivateUnitTestsToolWindow</IdeaActionID>
          <ShortCut>'Alt+8' or 'Alt+Umschalt+8'</ShortCut>
          <Description>Unit Tests</Description>
          <Count>2</Count>
        </Item>
      </Statistic>
    </option>
  </component>
</application>